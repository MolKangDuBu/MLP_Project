<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="Ocr">
<select id ="getStore_key" parameterType = "String" resultType ="String">
	select Store_key
	from Store
	where Store_bnumber = #{bnumber}
</select>
	
</mapper>